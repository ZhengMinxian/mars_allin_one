{
  "query": {
    "bool": {
      "filter": {
        "bool": {
          "must": [
              {"match": {"plugin": "cpu"}},
              {"match": {"collectd_type": "percent"}},
              {"match": {"type_instance": "system"}},
              {"range": {"@timestamp": {"gte": "now-15s"}}}
          ]
        }
      }
    }
  },
  "sort": [
    {
      "@timestamp": {
        "order": "desc"
      }
    }
  ]
}
